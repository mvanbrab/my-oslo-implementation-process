# Join Leeractiviteitspecificatie → instructietaal → Doelgroep
SELECT DISTINCT ?Leeractiviteitspecificatie_url ?Doelgroep_url WHERE {
  ?Leeractiviteitspecificatie_url a <http://data.europa.eu/snb/model/elm/LearningActivitySpecification> .
  ?Doelgroep_url a <http://www.w3.org/2004/02/skos/core#Concept> .
  ?Leeractiviteitspecificatie_url <http://purl.org/dc/terms/language> ?Doelgroep_url .
}
