# Join Leeractiviteitspecificatie → contacturen → Openingsurenspecificatie
SELECT DISTINCT ?Leeractiviteitspecificatie_url ?Openingsurenspecificatie_url WHERE {
  ?Leeractiviteitspecificatie_url a <http://data.europa.eu/snb/model/elm/LearningActivitySpecification> .
  ?Openingsurenspecificatie_url a <http://schema.org/OpeningHoursSpecification> .
  ?Leeractiviteitspecificatie_url <http://data.europa.eu/snb/model/elm/contactHour> ?Openingsurenspecificatie_url .
}
